{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Information"
      }
    },
    "IdentityServiceUrl": "http://identity-svc",
    "ReverseProxy": {
      "Clusters": {
        "posts": {
          "Destinations": {
            "postApi": {
              "Address": "http://post-svc"
            }
          }
        },
        "messages": {
          "Destinations": {
            "messageApi": {
              "Address": "http://post-svc"
            }
          }
        },"search": {
          "Destinations": {
            "searchApi": {
              "Address": "http://search-svc"
            }
          }
        },
        "notifications": {
          "Destinations": {
            "notifyApi": {
              "Address": "http://notify-svc"
            }
          }
        }
      }
    }
  }
  